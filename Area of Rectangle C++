#include<iostream>
using namespace std;

     class Shape
{ public:
int length, breadth, height, Rectangle;


void area();
void volume();

};
void rectangle::area()
{cout << length * breadth;
}

void rectanglevolume()
{cout << length * breadth * height;
}

int main ()
{
rectangle obj;
obj.length=10;
obj.breadth=8;
obj.height=4;
obj.area();
cout << endl;
obj.volume();


     return 0;
     }
